{
  "name": "taibul-ui",
  "version": "1.0.1",
  "private": false,
  "type": "module",
  "main": "./dist/taibul-ui.umd.js",
  "module": "./dist/taibul-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/taibul-ui.es.js",
      "require": "./dist/taibul-ui.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite serve example --config vite.config.ts",
    "build": "tsc && vite build",
    "build:example": "vite build --config vite.example.config.ts",
    "preview": "vite preview",
    "lint": "eslint",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9",
    "typescript": "^5",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^4.5.4"
  }
}
